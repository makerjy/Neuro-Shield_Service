# Core service identifiers
SERVICE_NAME=neuro-shield
# {{SERVICE_NAME}} default: neuro-shield

ENVIRONMENT=local
# {{ENVIRONMENT}} default: local (local/vm/cloud)

# Frontend
BASE_PATH=/neuro-shield/
# {{BASE_PATH}} default: /neuro-shield/
FRONT_DEV_PORT=5173
# {{FRONT_DEV_PORT}} default: 5173
WEB_HOST_PORT=8080
# {{WEB_HOST_PORT}} default: 8080

# Backend
API_PORT=8000
# {{API_PORT}} default: 8000
BACKEND_ENTRY=server_fastapi.app:app
# {{BACKEND_ENTRY}} default: server_fastapi.app:app

# PostgreSQL
DB_HOST_PORT=5433
# {{DB_HOST_PORT}} default: 5433
DB_NAME=neuro
# {{DB_NAME}} default: neuro
DB_USER=dbuser
# {{DB_USER}} default: dbuser
DB_PASSWORD=dbpass
# {{DB_PASSWORD}} default: dbpass
DATABASE_URL=postgresql://dbuser:dbpass@db:5432/neuro

# Model options
USE_MODEL=false
# {{USE_MODEL}} default: false
MODEL_PATH=./models/model.pkl
# {{MODEL_PATH}} optional
MODEL_GEN_PATH=./models/model_gen.pkl
# {{MODEL_GEN_PATH}} optional
MODEL_SCALER_PATH=./models/model_scaler.pkl
# {{MODEL_SCALER_PATH}} optional

# Demo options
DEMO_MODE=false
# {{DEMO_MODE}} optional
DEMO_RISK_SCALE=1.0
# {{DEMO_RISK_SCALE}} optional
DEMO_RISK_MAX=100
# {{DEMO_RISK_MAX}} optional

# Vite runtime build variable
VITE_BASE_PATH=/neuro-shield/
